<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Code_phone">


  <LinearLayout
      android:id="@+id/linearlayout7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:gravity="center"
      android:orientation="vertical"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent">

    <ImageView
        android:id="@+id/imageView5"
        style="@style/imageview_style3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        style="@style/constview_Style3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5">

      <TextView
          android:id="@+id/textView5"
          style="@style/textview_style3"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/textView4" />

      <TextView
          android:id="@+id/textView7"
          style="@style/textview_style4"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/textView6" />

      <TextView
          android:id="@+id/textView6"
          style="@style/textview_style5"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/textView5" />

      <TextView
          android:id="@+id/textView4"
          style="@style/textview_style6"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:id="@+id/linearlayoutsquares"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

      <ImageButton
          android:id="@+id/imageButton"
          style="@style/imagebutton_style2"
          android:background="#00FFFFFF"
          tools:layout_editor_absoluteX="63dp"
          tools:layout_editor_absoluteY="431dp" />

      <ImageButton
          android:id="@+id/imageButton2"
          style="@style/imagebutton_style2"
          android:background="#00FFFFFF"
          tools:layout_editor_absoluteX="110dp"
          tools:layout_editor_absoluteY="431dp" />

      <ImageButton
          android:id="@+id/imageButton33"
          style="@style/imagebutton_style2"
          android:background="#00FFFFFF"
          tools:layout_editor_absoluteX="157dp"
          tools:layout_editor_absoluteY="431dp" />

      <ImageButton
          android:id="@+id/imageButton4"
          style="@style/imagebutton_style2"
          android:background="#00FFFFFF"
          tools:layout_editor_absoluteX="204dp"
          tools:layout_editor_absoluteY="431dp" />

      <ImageButton
          android:id="@+id/imageButton5"
          style="@style/imagebutton_style2"
          android:background="#00FFFFFF"
          tools:layout_editor_absoluteX="251dp"
          tools:layout_editor_absoluteY="431dp" />

      <ImageButton
          android:id="@+id/imageButton6"
          style="@style/imagebutton_style2"
          android:background="#00FFFFFF"
          tools:layout_editor_absoluteX="298dp"
          tools:layout_editor_absoluteY="431dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView8"
        style="@style/textview_style7"
        android:layout_width="207dp"
        android:layout_height="21dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        style="@style/imagebutton_style3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="515dp" />
  </LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>

