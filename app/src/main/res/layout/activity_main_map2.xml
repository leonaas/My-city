<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_map2">


    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="411dp"
        android:layout_height="300dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.cardview.widget.CardView
        android:layout_width="411dp"
        android:layout_height="500dp"
        app:cardCornerRadius="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:id="@+id/liner_main0"
            android:layout_width="match_parent"
            android:layout_height="450dp"
            android:orientation="vertical"
            android:layout_gravity="center">

            <ImageView
                android:id="@+id/imageView25"
                android:layout_width="411dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/rectangle_32_1_" />

            <ImageView
                android:id="@+id/imageView27"
                android:layout_width="410dp"
                android:layout_height="10dp"
                app:srcCompat="@drawable/line_1"
                app:layout_constraintTop_toBottomOf="@id/imageView25"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"/>
            <LinearLayout
                android:id="@+id/liner2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView27">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="20dp"
                    android:layout_margin="10dp">

                    <ImageView
                        android:id="@+id/imageView29"
                        android:layout_width="110dp"
                        android:layout_height="73dp"
                        app:srcCompat="@drawable/bishkekvodokanal" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="20dp"
                    android:layout_margin="10dp">

                    <ImageView
                        android:id="@+id/imageView30"
                        android:layout_width="110dp"
                        android:layout_height="73dp"
                        app:srcCompat="@drawable/vodokanal" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="20dp"
                    android:layout_margin="10dp">

                    <ImageView
                        android:id="@+id/imageView31"
                        android:layout_width="110dp"
                        android:layout_height="73dp"
                        app:srcCompat="@drawable/vodokanal_2" />
                </androidx.cardview.widget.CardView>

            </LinearLayout>
            <ImageView
                android:id="@+id/imageView28"
                android:layout_width="410dp"
                android:layout_height="10dp"
                app:srcCompat="@drawable/line_1"
                app:layout_constraintTop_toBottomOf="@id/liner2"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"/>

            <LinearLayout
                android:id="@+id/liner0"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginTop="10dp">

                <LinearLayout
                    android:id="@+id/liner6"
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView28">

                    <ImageView
                        android:id="@+id/imageView32"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/_643779_clock_hour_ticker_time_times_113446_2"
                        tools:layout_editor_absoluteX="131dp"
                        tools:layout_editor_absoluteY="184dp" />

                    <LinearLayout
                        android:id="@+id/liner3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:orientation="vertical"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/imageView28">

                        <TextView
                            style="@style/textview_style_map"
                            android:id="@+id/textView19"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/time_of_work"
                            />

                        <TextView
                            android:id="@+id/textView20"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/time_for_lunch"
                            app:layout_constraintBottom_toTopOf="@id/textView19" />

                    </LinearLayout>
                </LinearLayout>
                <ImageView
                    android:id="@+id/imageView36"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@+id/liner6"
                    app:srcCompat="@drawable/line_6"
                    android:layout_marginBottom="20dp"/>

                <LinearLayout
                    android:id="@+id/liner7"
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView36">

                    <ImageView
                        android:id="@+id/imageView33"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/location_1"
                        tools:layout_editor_absoluteX="131dp"
                        tools:layout_editor_absoluteY="184dp" />

                    <LinearLayout
                        android:id="@+id/liner8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:orientation="vertical"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/imageView28">

                        <TextView
                            style="@style/textview_style_map"
                            android:id="@+id/textView22"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/adres"
                            tools:layout_editor_absoluteX="213dp"
                            tools:layout_editor_absoluteY="192dp" />

                        <TextView
                            android:id="@+id/textView21"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/mail"
                            app:layout_constraintBottom_toTopOf="@id/textView19"
                            tools:layout_editor_absoluteX="213dp"
                            tools:layout_editor_absoluteY="175dp" />

                    </LinearLayout>
                </LinearLayout>
                <ImageView
                    android:id="@+id/imageView37"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@+id/liner7"
                    app:srcCompat="@drawable/line_6"
                    android:layout_marginBottom="20dp"/>

                <LinearLayout
                    android:id="@+id/liner9"
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/liner7">

                    <ImageView
                        android:id="@+id/imageView34"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/world"
                        tools:layout_editor_absoluteX="169dp"
                        tools:layout_editor_absoluteY="275dp"/>

                    <TextView
                        style="@style/textview_style_map"
                        android:id="@+id/textView23"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/websites"
                        app:layout_constraintStart_toEndOf="@id/imageView34"
                        tools:layout_editor_absoluteX="205dp"
                        tools:layout_editor_absoluteY="279dp"/>

                </LinearLayout>
                <ImageView
                    android:id="@+id/imageView38"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@+id/liner9"
                    app:srcCompat="@drawable/line_6"
                    android:layout_marginBottom="20dp"/>

                <LinearLayout
                    android:id="@+id/liner10"
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/liner9">

                    <ImageView
                        android:id="@+id/imageView35"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/phone_call_icon_128788" />

                    <TextView
                        style="@style/textview_style_map"
                        android:id="@+id/textView24"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/number_of_organization"
                        app:layout_constraintStart_toEndOf="@id/imageView34"/>

                </LinearLayout>
                <ImageView
                    android:id="@+id/imageView39"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@+id/liner10"
                    app:srcCompat="@drawable/line_6"/>

            </LinearLayout>


        </LinearLayout>

    </androidx.cardview.widget.CardView>




</androidx.constraintlayout.widget.ConstraintLayout>